/* Console Styles */
.error-badge {
  background-color: #dc3545;
  color: white;
  border-radius: 50%;
  padding: 2px 6px;
  font-size: 0.7rem;
  margin-left: 5px;
  display: inline-block;
  min-width: 18px;
  text-align: center;
  line-height: 1.2;
}

.console-container {
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: #1e1e1e;
  border-radius: 6px;
  overflow: hidden;
  font-family: 'Courier New', monospace;
}

.console-header {
  background-color: #2d2d2d;
  color: #fff;
  padding: 10px 15px;
  border-bottom: 1px solid #444;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 0.9rem;
}

.console-content {
  flex: 1;
  overflow-y: auto;
  padding: 0;
  background-color: #1e1e1e;
  max-height: calc(80vh - 200px);
}

.console-empty {
  padding: 40px 20px;
  text-align: center;
  color: #888;
}

.console-entry {
  padding: 8px 15px;
  border-bottom: 1px solid #333;
  font-size: 0.85rem;
  display: grid;
  grid-template-columns: 140px 80px 1fr;
  gap: 10px;
  align-items: start;
}

.console-entry:hover {
  background-color: #252525;
}

.console-error {
  border-left: 3px solid #ff6b6b;
  background-color: rgba(255, 107, 107, 0.05);
}

.console-warning {
  border-left: 3px solid #ffa500;
  background-color: rgba(255, 165, 0, 0.05);
}

.console-timestamp {
  color: #888;
  font-size: 0.8rem;
  white-space: nowrap;
}

.console-type {
  color: #fff;
  font-weight: bold;
  text-transform: uppercase;
  font-size: 0.75rem;
}

.console-error .console-type {
  color: #ff6b6b;
}

.console-warning .console-type {
  color: #ffa500;
}

.console-message {
  color: #e6e6e6;
  white-space: pre-wrap;
  word-break: break-word;
  line-height: 1.4;
}

.btn-sm {
  padding: 5px 10px;
  font-size: 0.8rem;
}

