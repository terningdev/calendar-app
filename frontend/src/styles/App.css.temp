}

/* Additional left padding/margin removal for perfect alignment */
.calendar-week .rbc-event,
.calendar-week .rbc-row-segment .rbc-event,
.calendar-week .rbc-allday-cell .rbc-event {
  margin-left: 0 !important;
  padding-left: 0 !important;
  left: 0 !important;
}

.calendar-day .rbc-event,
