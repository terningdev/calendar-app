/* Custom Calendar Toolbar */
.custom-calendar-toolbar {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 15px 20px;
  background-color: #f8f9fa;
  border-bottom: 1px solid #e9ecef;
  margin-bottom: 10px;
}

.toolbar-left {
  display: flex;
  align-items: center;
  gap: 10px;
}

.toolbar-center {
  flex: 1;
  text-align: center;
}

.toolbar-label {
  font-size: 1.2rem;
  font-weight: 600;
  color: #2c3e50;
}

.toolbar-right {
  display: flex;
  align-items: center;
  gap: 2px;
  margin-left: 10px;
}

.toolbar-btn {
  padding: 8px 12px;
  border: 1px solid #dee2e6;
  background-color: white;
  color: #495057;
