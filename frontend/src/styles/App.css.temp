   ======================================== */

.mobile-search-overlay {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 1000;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  padding-top: 20px;
}

.mobile-search-container {
  background: white;
  border-radius: 8px;
  padding: 12px;
  box-shadow: 0 4px 20px rgba(0,0,0,0.15);
  width: 90%;
  max-width: 400px;
  position: sticky;
  top: 20px;
}

.mobile-search-input-simple {
  width: 100%;
  padding: 12px 16px;
  border: 2px solid #e9ecef;
  border-radius: 6px;
  font-size: 16px; /* Prevents zoom on iOS */
  background: #f8f9fa;
  transition: all 0.3s ease;
}

.mobile-search-input-simple:focus {
  outline: none;
  border-color: #007bff;
  background: white;
  box-shadow: 0 0 0 3px rgba(0,123,255,0.15);
}

/* Mobile Responsive - Switch to popup system */
@media (max-width: 768px) {
  .desktop-selector {
    display: none;
  }
  
  .mobile-selector {
    display: block;
  }
}
