.calendar-day .rbc-allday-cell .rbc-event {
  margin-left: 0 !important;
  padding-left: 0 !important;
  left: 0 !important;
}

/* All events spanning more than one day should be above separator lines */
.calendar-week .rbc-event[style*="width"][style*="%"] {
  z-index: 3 !important;
  position: relative !important;
}

/* Specifically target events wider than ~15% (more than one day column) */
